{
  "routes": [
    {
      "src": "^/assets/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "dest": "/assets/$1"
    },
    {
      "src": "^/favicon\\.ico$",
      "dest": "/favicon.ico"
    },
    {
      "src": "^/admin/login$",
      "dest": "/index.html"
    },
    {
      "src": ".*",
      "status": 404
    }
  ]
}
