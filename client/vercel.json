{
  "routes": [
    {
      "src": "^/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "^/(favicon\\.ico|robots\\.txt)$",
      "dest": "/$1"
    },
    {
      "src": "^/admin/login$",
      "dest": "/index.html"
    },
    {
      "src": "^/index\\.html$",
      "dest": "/index.html"
    },
    {
      "src": ".*",
      "status": 404
    }
  ]
}
